{
  "name": "daks-svelte",
  "version": "0.1.48",
  "description": "DAKS Svelte PKG",
  "repository": {
    "type": "git",
    "url": "https://github.com/daks-dev/daks-svelte"
  },
  "author": {
    "name": "DVA",
    "email": "mail@daks.dev",
    "url": "https://daks.dev"
  },
  "bugs": "https://github.com/daks-dev/daks-svelte/issues",
  "homepage": "https://svelte.daks.dev/",
  "keywords": [
    "svelte",
    "sveltekit",
    "tailwindcss",
    "carousel",
    "dropdown",
    "iconify",
    "imagetools",
    "footer",
    "lightbox",
    "link",
    "navbar",
    "select",
    "slider",
    "spinner",
    "vite-plugin",
    "yandex"
  ],
  "private": false,
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev --port=3000",
    "build": "vite build",
    "preview": "vite preview --port=3000",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package && publint",
    "test": "playwright test",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "peerDependencies": {
    "@iconify/svelte": "^3.1.0",
    "@sveltejs/kit": "^1.11.0",
    "@types/html-minifier-terser": "^7.0.0",
    "classnames": "^2.3.2",
    "html-minifier-terser": "^7.1.0",
    "iconify-icon": "^1.0.7",
    "svelte": "^3.56.0"
  },
  "devDependencies": {
    "@iconify/svelte": "^3.1.0",
    "@playwright/test": "^1.31.2",
    "@sveltejs/adapter-node": "^1.2.2",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.11.0",
    "@sveltejs/package": "^2.0.2",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "cssnano": "^5.1.15",
    "cssnano-preset-advanced": "^5.3.10",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "esm-env": "^1.0.0",
    "html-minifier-terser": "^7.1.0",
    "iconify-icon": "^1.0.7",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "publint": "^0.1.10",
    "sharp": "^0.31.3",
    "svelte": "^3.56.0",
    "svelte-check": "^3.1.2",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vanilla-lazyload": "^17.8.3",
    "vite": "^4.1.4",
    "vite-imagetools": "^4.0.18",
    "vitest": "^0.29.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./vite/*": {
      "types": "./dist/vite/*.d.ts",
      "default": "./dist/vite/*.js"
    },
    "./tailwindcss": "./dist/assets/tailwindcss/index.css",
    "./tailwindcss/*": "./dist/assets/tailwindcss/*",
    "./fonts/*": "./dist/assets/fonts/*.css",
    "./icons/*": "./dist/assets/icons/*",
    "./images/*": "./dist/assets/images/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    ">4.0": {
      "server": [
        "./dist/server.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ],
      "vite/*": [
        "./dist/vite/*.d.ts"
      ]
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}